\ProvideDocumentCommand{\method}{}{\ensuremath{.\!}}
\ProvideDocumentCommand{\NewScheme}{m m}{%
  \ProvideDocumentCommand{#1}{s o}{%
    \ensuremath{%
      \operatorname{%
        \IfBooleanTF{##1}{%
          \overline{{\mathsf{#2}}}%
        }{%
          {\mathsf{#2}}%
        }%
        \IfValueT{##2}{.\!\mathsf{##2}}%
      }%
    }%
  }%
}
\ProvideDocumentCommand{\NewAlgorithm}{m m}{%
  \ProvideDocumentCommand{#1}{s o o}{%
    \ensuremath{%
      \operatorname{%
        \IfBooleanTF{##1}{%
          \overline{\mathsf{#2}}%
        }{%
          \mathsf{#2}%
        }%
      }%
      \IfValueTF{##3}{%
        ##2%
        \mleft(##3\mright)%
      }{%
        \IfValueT{##2}{\mleft(##2\mright)}%
      }%
    }%
  }%
}
\ProvideDocumentCommand{\NewFunction}{m m}{%
  \ProvideDocumentCommand{#1}{s o o}{%
    \ensuremath{%
      \operatorname{%
        \IfBooleanTF{##1}{%
          \overline{#2}%
        }{%
          #2%
        }%
      }%
      \IfValueTF{##3}{%
        ##2%
        \mleft(##3\mright)%
      }{%
        \IfValueT{##2}{\mleft(##2\mright)}%
      }%
    }%
  }%
}
\ProvideDocumentCommand{\NewSet}{m m}{%
  \ProvideDocumentCommand{#1}{}{%
    \ensuremath{\mathcal{#2}}%
  }%
}
\ProvideDocumentCommand{\NewEntity}{m m}{%
  \ProvideDocumentCommand{#1}{}{%
    \ensuremath{\mathcal{#2}}%
  }%
}
\ProvideDocumentCommand{\NewVariable}{m m}{%
  \ProvideDocumentCommand{#1}{}{%
    \ensuremath{\mathit{#2}}%
  }
}
%
% Proofs of knowledge
%
\ProvideDocumentCommand{\PK}{o o}{\ensuremath{%
    \operatorname{PK}%
    \IfValueT{#1}{%
      \IfValueTF{#2}{%
        \mleft\{\mleft(#1\mright) : #2\mright\}%
      }{%
        \mleft\{#1\mright\}%
      }%
    }%
  }}
\ProvideDocumentCommand{\SPK}{o o o}{\ensuremath{%
    \operatorname{SPK}%
    \IfValueT{#1}{%
      \IfValueTF{#2}{%
        \mleft\{\mleft(#1\mright) : #2\mright\}%
      }{%
        \mleft\{#1\mright\}%
      }%
      \IfValueT{#3}{%
        \mleft(#3\mright)%
      }%
    }%
  }}
